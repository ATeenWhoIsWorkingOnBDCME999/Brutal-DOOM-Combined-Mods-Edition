ACTOR HitSparkPlasma: HitSpark
{
 speed 8
  Scale 0.06
  Gravity 1.0
  BounceType "None"
  States
    {
     Spawn:
	   SPKO S 40 BRIGHT
       SPKO SSSSSSSSSS 1 Bright 
	   {
		A_FadeOut(0.1);
		return A_CheckFloor("Death");
	   }
	   stop
		}
}