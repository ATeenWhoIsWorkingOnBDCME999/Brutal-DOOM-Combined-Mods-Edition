ACTOR HitSpark1: ExplosionParticleHeavy replaces HitSpark
{
 speed 10
  Scale 0.02
 Gravity 0.7
 BounceType "Doom"
 +USEBOUNCESTATE
	-BOUNCEONFLOORS
	-NOBLOCKMAP
	+BOUNCELIKEHERETIC
	-NOCLIP
   
   BOUNCEFACTOR 0.5
    States
    {
     Spawn:
	   SPKO S 20 BRIGHT
       SPKO SSSSSSSSSS 1 Bright A_FadeOut(0.1)
        stop
	Bounce.Floor:
     Death:
     TNT1 A 1
       Stop
    }
}

